{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "rules": {
    "no-underscore-dangle": ["error",{"allow":["_id"]}],
    "no-control-regex": 0,
    "no-unused-vars": 0,
    // выдавал ошибки по регулярному выражению \. и \/ 
    "no-useless-escape": 0

  }
}
